<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay480_12120"
			editable="false"
			layout="ResponsiveGridLayout"
			labelSpanXL="4"
			labelSpanL="4"
			labelSpanM="12"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="2"
			columnsL="2"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<core:Title text="{products>ProductName}" />
				<Image class="productImage detailImage sapUiMediumMarginBottom" src="{products>ImageUrl}"/>
				<Label text="Ürün Kodu" />
				<Text text="{products>ProductCode}" />
				<Label text="Miktar" />
				<Text text="{products>Quantity}" />
				<Label text="Birim Fiyatı" />
				<Text text="{products>ExtendedPrice} TL" />
				<Label text="Beden" />
				<Text text="{products>SizeId}" />
				<Label text="Renk" />
				<Text text="{products>ColorId}" />
				<Label text="Alt Kategori" />
				<Text text="{products>SubCategoryId}" />
				<Label text="Kategori" />
				<Text text="{products>CategoryId}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>